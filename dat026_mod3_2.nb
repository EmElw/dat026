  RowBox[{"(*", " ", 
   RowBox[{
   "This", " ", "is", " ", "how", " ", "often", " ", "the", " ", "signal", 
    " ", "is", " ", 
    RowBox[{"sampled", ".", " ", "Do"}], " ", "not", " ", 
    RowBox[{"change", "!"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"xSine", "[", 
    RowBox[{"freq_", ",", "n_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Sin", "[", 
